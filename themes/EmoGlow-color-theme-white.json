    "notificationCenterHeader.foreground": "#000000",
    "notificationToast.border": "#e6e6e6",
    "notifications.foreground": "#000000",
    "notifications.background": "#ffffff",
    "notifications.border": "#e6e6e6",
    "notificationLink.foreground": "#82ecff",
    "extensionButton.prominentBackground": "#ffffff"
